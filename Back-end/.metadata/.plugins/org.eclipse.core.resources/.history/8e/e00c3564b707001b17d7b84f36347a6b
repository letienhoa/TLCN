package carbook.entity;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="ve")
public class Ve {

	@Id
	private int id;
	
	@Column(name="gio_chay")
	private String gioChay;
	
	private String gioKetThuc;
	
	private int idXe;
	
	private String sdtKhachHang;
	
	private String email;
	
	private String 	code;
	
	public Ve() {
		
	}
	
	
}
