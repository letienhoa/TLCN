package carbook.daoimpl;

import java.util.List;

import javax.persistence.ParameterMode;

import org.hibernate.procedure.ProcedureCall;

import carbook.dao.AbstractDao;
import carbook.dao.GiuongDao;
import carbook.entity.GiuongModelData;

public class GiuongDaoImpl extends AbstractDao<Integer,GiuongModelData>  implements GiuongDao {

	@Override
	public List<GiuongModelData> spGetGiuongByTuyenXeId(int tuyenXeId, int gio) {
		ProcedureCall procedureCall = this.getSession().createStoredProcedureCall("sp_get_giuong_by_tuyen_xe",GiuongModelData.class);
		procedureCall.registerParameter("id_tuyen_xe", Integer.class, ParameterMode.IN).bindValue(tuyenXeId);
				List<Integer> list =procedureCall.getResultList();
		return null;
	}
	
}
