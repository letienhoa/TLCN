package carbook.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="ben")
public class Ben {

	@Id
	@Column(name="id")
	private int id;
	
	@Column(name="ten_ben")
	private String tenBen;
	
	@Column(name="dia_chi")
	private String diaChi;
	
	public Ben() {
		
	}
	
	
}
