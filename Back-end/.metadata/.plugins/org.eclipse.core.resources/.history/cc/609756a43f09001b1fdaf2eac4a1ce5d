package carbook.entity;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="xe")
public class Xe {

	@Id
	private int id;
	
	private String tenXe;
	
	private String hangXe;
	
	private int tuyenSanSangId;
	
	private int tuyenOffId;
	
	private int gioChay;
	
}
